{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "dataLakeName": {
            "type": "string"
        },
        "blobStorageName": {
            "type": "string"
        },
        "sqlServerName": {
            "type": "string"
        },
        "dataFactoryName": {
            "type": "string"
        },
        "keyVaultName": {
            "type": "string"
        },
        "subnetId": {
            "type": "string"
        },
        "location": {
            "type": "string",
            "defaultValue": "Australia East",
            "allowedValues": [
                "Australia East",
                "Australia Southeast",
                "australiaeast",
                "australiasoutheast"
            ],
            "metadata": {
                "description": "Specifies the location for the resource"
            }
        }
    },
    "variables": {
        
    },
    "resources": [
        {
            "apiVersion": "2019-04-01",
            "name": "[concat('pe-dfs-', parameters('dataLakeName'))]",
            "type": "Microsoft.Network/privateEndpoints",
            "location": "[parameters('location')]",
            "properties": {
                "privateLinkServiceConnections": [
                    {
                        "name": "[concat('pe-dfs-conn-', parameters('dataLakeName'))]",
                        "properties": {
                            "privateLinkServiceId": "[resourceId('Microsoft.Storage/storageAccounts', parameters('dataLakeName'))]",
                            "groupIds": ["dfs"]
                        }
                    }
                ],
                "manualPrivateLinkServiceConnections": [],
                "subnet": {
                    "id": "[parameters('subnetId')]"
                }
            }
        },
        {
            "apiVersion": "2019-04-01",
            "name": "[concat('pe-blob-', parameters('dataLakeName'))]",
            "type": "Microsoft.Network/privateEndpoints",
            "location": "[parameters('location')]",
            "properties": {
                "privateLinkServiceConnections": [
                    {
                        "name": "[concat('pe-dfs-conn-', parameters('dataLakeName'))]",
                        "properties": {
                            "privateLinkServiceId": "[resourceId('Microsoft.Storage/storageAccounts', parameters('dataLakeName'))]",
                            "groupIds": ["blob"]
                        }
                    }
                ],
                "manualPrivateLinkServiceConnections": [],
                "subnet": {
                    "id": "[parameters('subnetId')]"
                }
            }
        },
        {
            "apiVersion": "2019-04-01",
            "name": "[concat('pe-blob-', parameters('blobStorageName'))]",
            "type": "Microsoft.Network/privateEndpoints",
            "location": "[parameters('location')]",
            "properties": {
                "privateLinkServiceConnections": [
                    {
                        "name": "[concat('pe-conn-', parameters('blobStorageName'))]",
                        "properties": {
                            "privateLinkServiceId": "[resourceId('Microsoft.Storage/storageAccounts', parameters('blobStorageName'))]",
                            "groupIds": ["blob"]
                        }
                    }
                ],
                "manualPrivateLinkServiceConnections": [],
                "subnet": {
                    "id": "[parameters('subnetId')]"
                }
            }
        },
        {
            "apiVersion": "2019-04-01",
            "name": "[concat('pe-sqlserver-', parameters('sqlServerName'))]",
            "type": "Microsoft.Network/privateEndpoints",
            "location": "[parameters('location')]",
            "properties": {
                "privateLinkServiceConnections": [
                    {
                        "name": "[concat('pe-conn-', parameters('sqlServerName'))]",
                        "properties": {
                            "privateLinkServiceId": "[resourceId('Microsoft.Sql/servers', parameters('sqlServerName'))]",
                            "groupIds": ["sqlServer"]
                        }
                    }
                ],
                "manualPrivateLinkServiceConnections": [],
                "subnet": {
                    "id": "[parameters('subnetId')]"
                }
            }
        },
        {
            "apiVersion": "2019-04-01",
            "name": "[concat('pe-datafactory-', parameters('dataFactoryName'))]",
            "type": "Microsoft.Network/privateEndpoints",
            "location": "[parameters('location')]",
            "properties": {
                "privateLinkServiceConnections": [
                    {
                        "name": "[concat('pe-conn-', parameters('dataFactoryName'))]",
                        "properties": {
                            "privateLinkServiceId": "[resourceId('Microsoft.DataFactory/factories', parameters('dataFactoryName'))]",
                            "groupIds": ["dataFactory"]
                        }
                    }
                ],
                "manualPrivateLinkServiceConnections": [],
                "subnet": {
                    "id": "[parameters('subnetId')]"
                }
            }
        },
        {
            "apiVersion": "2019-04-01",
            "name": "[concat('pe-vault-', parameters('keyVaultName'))]",
            "type": "Microsoft.Network/privateEndpoints",
            "location": "[parameters('location')]",
            "properties": {
                "privateLinkServiceConnections": [
                    {
                        "name": "[concat('pe-conn-', parameters('keyVaultName'))]",
                        "properties": {
                            "privateLinkServiceId": "[resourceId('Microsoft.KeyVault/vaults', parameters('keyVaultName'))]",
                            "groupIds": ["vault"]
                        }
                    }
                ],
                "manualPrivateLinkServiceConnections": [],
                "subnet": {
                    "id": "[parameters('subnetId')]"
                }
            }
        }
    ],
    "outputs": {
    }
}
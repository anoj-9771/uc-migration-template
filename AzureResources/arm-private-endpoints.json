{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "dataLakeName": {
            "type": "string"
        },
        "blobStorageName": {
            "type": "string"
        },
        "sqlServerName": {
            "type": "string"
        },
        "dataFactoryName": {
            "type": "string"
        },
        "keyVaultName": {
            "type": "string"
        },
        "synapseWorkspaceName": {
            "type": "string"
        },
        "subnetId": {
            "type": "string"
        },
        "location": {
            "type": "string",
            "defaultValue": "Australia East",
            "allowedValues": [
                "Australia East",
                "Australia Southeast",
                "australiaeast",
                "australiasoutheast"
            ],
            "metadata": {
                "description": "Specifies the location for the resource"
            }
        }
    },
    "resources": [
        {
            "apiVersion": "2019-04-01",
            "name": "[concat(parameters('dataLakeName'), '-pe')]",
            "type": "Microsoft.Network/privateEndpoints",
            "location": "[parameters('location')]",
            "properties": {
                "privateLinkServiceConnections": [
                    {
                        "name": "[concat(parameters('dataLakeName'), '-pe-conn')]",
                        "properties": {
                            "privateLinkServiceId": "[resourceId('Microsoft.Storage/storageAccounts', parameters('dataLakeName'))]",
                            "groupIds": ["dfs"]
                        }
                    }
                ],
                "manualPrivateLinkServiceConnections": [],
                "subnet": {
                    "id": "[parameters('subnetId')]"
                }
            }
        },
        {
            "apiVersion": "2019-04-01",
            "name": "[concat(parameters('blobStorageName'), '-pe')]",
            "type": "Microsoft.Network/privateEndpoints",
            "location": "[parameters('location')]",
            "properties": {
                "privateLinkServiceConnections": [
                    {
                        "name": "[concat(parameters('blobStorageName'), '-pe-conn')]",
                        "properties": {
                            "privateLinkServiceId": "[resourceId('Microsoft.Storage/storageAccounts', parameters('blobStorageName'))]",
                            "groupIds": ["blob"]
                        }
                    }
                ],
                "manualPrivateLinkServiceConnections": [],
                "subnet": {
                    "id": "[parameters('subnetId')]"
                }
            }
        },
        {
            "apiVersion": "2019-04-01",
            "name": "[concat(parameters('sqlServerName'), '-pe')]",
            "type": "Microsoft.Network/privateEndpoints",
            "location": "[parameters('location')]",
            "properties": {
                "privateLinkServiceConnections": [
                    {
                        "name": "[concat(parameters('sqlServerName'), '-pe-conn')]",
                        "properties": {
                            "privateLinkServiceId": "[resourceId('Microsoft.Sql/servers', parameters('sqlServerName'))]",
                            "groupIds": ["sqlServer"]
                        }
                    }
                ],
                "manualPrivateLinkServiceConnections": [],
                "subnet": {
                    "id": "[parameters('subnetId')]"
                }
            }
        },
        {
            "apiVersion": "2019-04-01",
            "name": "[concat(parameters('dataFactoryName'), '-pe')]",
            "type": "Microsoft.Network/privateEndpoints",
            "location": "[parameters('location')]",
            "properties": {
                "privateLinkServiceConnections": [
                    {
                        "name": "[concat(parameters('dataFactoryName'), '-pe-conn')]",
                        "properties": {
                            "privateLinkServiceId": "[resourceId('Microsoft.DataFactory/factories', parameters('dataFactoryName'))]",
                            "groupIds": ["dataFactory"]
                        }
                    }
                ],
                "manualPrivateLinkServiceConnections": [],
                "subnet": {
                    "id": "[parameters('subnetId')]"
                }
            }
        },
        {
            "apiVersion": "2019-04-01",
            "name": "[concat(parameters('keyVaultName'), '-pe')]",
            "type": "Microsoft.Network/privateEndpoints",
            "location": "[parameters('location')]",
            "properties": {
                "privateLinkServiceConnections": [
                    {
                        "name": "[concat(parameters('keyVaultName'), '-pe-conn')]",
                        "properties": {
                            "privateLinkServiceId": "[resourceId('Microsoft.KeyVault/vaults', parameters('keyVaultName'))]",
                            "groupIds": ["vault"]
                        }
                    }
                ],
                "manualPrivateLinkServiceConnections": [],
                "subnet": {
                    "id": "[parameters('subnetId')]"
                }
            }
        },
        {
            "apiVersion": "2019-04-01",
            "name": "[concat(parameters('synapseWorkspaceName'), '-pe-sql')]",
            "type": "Microsoft.Network/privateEndpoints",
            "location": "[parameters('location')]",
            "properties": {
                "privateLinkServiceConnections": [
                    {
                        "name": "[concat(parameters('synapseWorkspaceName'), '-pe-sql-conn')]",
                        "properties": {
                            "privateLinkServiceId": "[resourceId('Microsoft.Synapse/workspaces', parameters('synapseWorkspaceName'))]",
                            "groupIds": ["sql"]
                        }
                    }
                ],
                "manualPrivateLinkServiceConnections": [],
                "subnet": {
                    "id": "[parameters('subnetId')]"
                }
            }
        },
        {
            "apiVersion": "2019-04-01",
            "name": "[concat(parameters('synapseWorkspaceName'), '-pe-sqlondemand')]",
            "type": "Microsoft.Network/privateEndpoints",
            "location": "[parameters('location')]",
            "properties": {
                "privateLinkServiceConnections": [
                    {
                        "name": "[concat(parameters('synapseWorkspaceName'), '-pe-sqlondemand-conn')]",
                        "properties": {
                            "privateLinkServiceId": "[resourceId('Microsoft.Synapse/workspaces', parameters('synapseWorkspaceName'))]",
                            "groupIds": ["sqlondemand"]
                        }
                    }
                ],
                "manualPrivateLinkServiceConnections": [],
                "subnet": {
                    "id": "[parameters('subnetId')]"
                }
            }
        },
        {
            "apiVersion": "2019-04-01",
            "name": "[concat(parameters('synapseWorkspaceName'), '-pe-dev')]",
            "type": "Microsoft.Network/privateEndpoints",
            "location": "[parameters('location')]",
            "properties": {
                "privateLinkServiceConnections": [
                    {
                        "name": "[concat(parameters('synapseWorkspaceName'), '-pe-dev-conn')]",
                        "properties": {
                            "privateLinkServiceId": "[resourceId('Microsoft.Synapse/workspaces', parameters('synapseWorkspaceName'))]",
                            "groupIds": ["dev"]
                        }
                    }
                ],
                "manualPrivateLinkServiceConnections": [],
                "subnet": {
                    "id": "[parameters('subnetId')]"
                }
            }
        },
        {
            "type": "Microsoft.Synapse/privateLinkHubs",
            "apiVersion": "2021-06-01-preview",
            "name": "[concat(toLower(replace(parameters('synapseWorkspaceName'),'-','')),'ph')]",
            "location": "[parameters('location')]",
            "properties": {}
        }
    ],
    "outputs": {
    }
}
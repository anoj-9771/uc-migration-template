{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "dataLakeName": {
            "type": "string"
        },
        "blobStorageName": {
            "type": "string"
        },
        "sqlServerName": {
            "type": "string"
        },
        "dataFactoryName": {
            "type": "string"
        },
        "keyVaultName": {
            "type": "string"
        },
        "subnetId": {
            "type": "string"
        }
    },
    "resources": [
        {
            "apiVersion": "2019-04-01",
            "name": "[concat(parameters('dataLakeName'), '-pe')]",
            "type": "Microsoft.Network/privateEndpoints",
            "location": "[resourceGroup().Location]",
            "properties": {
                "privateLinkServiceConnections": [
                    {
                        "name": "[concat(parameters('dataLakeName'), '-pe-conn')]",
                        "properties": {
                            "privateLinkServiceId": "[resourceId('Microsoft.Storage/storageAccounts', parameters('dataLakeName'))]",
                            "groupIds": ["dfs"]
                        }
                    }
                ],
                "manualPrivateLinkServiceConnections": [],
                "subnet": {
                    "id": "[parameters('subnetId')]"
                }
            }
        },
        {
            "apiVersion": "2019-04-01",
            "name": "[concat(parameters('blobStorageName'), '-pe')]",
            "type": "Microsoft.Network/privateEndpoints",
            "location": "[resourceGroup().Location]",
            "properties": {
                "privateLinkServiceConnections": [
                    {
                        "name": "[concat(parameters('blobStorageName'), '-pe-conn')]",
                        "properties": {
                            "privateLinkServiceId": "[resourceId('Microsoft.Storage/storageAccounts', parameters('blobStorageName'))]",
                            "groupIds": ["blob"]
                        }
                    }
                ],
                "manualPrivateLinkServiceConnections": [],
                "subnet": {
                    "id": "[parameters('subnetId')]"
                }
            }
        },
        {
            "apiVersion": "2019-04-01",
            "name": "[concat(parameters('sqlServerName'), '-pe')]",
            "type": "Microsoft.Network/privateEndpoints",
            "location": "[resourceGroup().Location]",
            "properties": {
                "privateLinkServiceConnections": [
                    {
                        "name": "[concat(parameters('sqlServerName'), '-pe-conn')]",
                        "properties": {
                            "privateLinkServiceId": "[resourceId('Microsoft.Sql/servers', parameters('sqlServerName'))]",
                            "groupIds": ["sqlServer"]
                        }
                    }
                ],
                "manualPrivateLinkServiceConnections": [],
                "subnet": {
                    "id": "[parameters('subnetId')]"
                }
            }
        },
        {
            "apiVersion": "2019-04-01",
            "name": "[concat(parameters('dataFactoryName'), '-pe')]",
            "type": "Microsoft.Network/privateEndpoints",
            "location": "[resourceGroup().Location]",
            "properties": {
                "privateLinkServiceConnections": [
                    {
                        "name": "[concat(parameters('dataFactoryName'), '-pe-conn')]",
                        "properties": {
                            "privateLinkServiceId": "[resourceId('Microsoft.DataFactory/factories', parameters('dataFactoryName'))]",
                            "groupIds": ["dataFactory"]
                        }
                    }
                ],
                "manualPrivateLinkServiceConnections": [],
                "subnet": {
                    "id": "[parameters('subnetId')]"
                }
            }
        },
        {
            "apiVersion": "2019-04-01",
            "name": "[concat(parameters('keyVaultName'), '-pe')]",
            "type": "Microsoft.Network/privateEndpoints",
            "location": "[resourceGroup().Location]",
            "properties": {
                "privateLinkServiceConnections": [
                    {
                        "name": "[concat(parameters('keyVaultName'), '-pe-conn')]",
                        "properties": {
                            "privateLinkServiceId": "[resourceId('Microsoft.KeyVault/vaults', parameters('keyVaultName'))]",
                            "groupIds": ["vault"]
                        }
                    }
                ],
                "manualPrivateLinkServiceConnections": [],
                "subnet": {
                    "id": "[parameters('subnetId')]"
                }
            }
        }
    ],
    "outputs": {
    }
}
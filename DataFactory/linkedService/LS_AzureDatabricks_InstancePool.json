{
	"name": "LS_AzureDatabricks_InstancePool",
	"properties": {
		"parameters": {
			"kvSecret": {
				"type": "string",
				"defaultValue": "databricks-token"
			},
			"PoolId": {
				"type": "string",
				"defaultValue": "0121-060302-next337-pool-owblzzx4"
			},
			"DatabricksVersion": {
				"type": "string",
				"defaultValue": "12.2.x-scala2.12"
			},
			"newClusterNumOfWorker": {
				"type": "string",
				"defaultValue": "1:8"
			},
			"ProductTag": {
				"type": "string",
				"defaultValue": "Framework ETL"
			}
		},
		"annotations": [],
		"type": "AzureDatabricks",
		"typeProperties": {
			"domain": "https://australiaeast.azuredatabricks.net",
			"accessToken": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "LS-AzureKeyVault",
					"type": "LinkedServiceReference"
				},
				"secretName": {
					"value": "@linkedService().kvSecret",
					"type": "Expression"
				}
			},
			"instancePoolId": "@linkedService().PoolId",
			"newClusterNumOfWorker": "@linkedService().newClusterNumOfWorker",
			"newClusterSparkConf": {
				" spark.sql.session.timeZone": "Australia/Sydney"
			},
			"newClusterSparkEnvVars": {
				"PYSPARK_PYTHON": "/databricks/python3/bin/python3"
			},
			"newClusterCustomTags": {
				"Product": {
					"value": "@linkedService().ProductTag",
					"type": "Expression"
				}
			},
			"newClusterVersion": "@linkedService().DatabricksVersion",
			"newClusterInitScripts": [],
			"clusterOption": "Autoscaling"
		},
		"connectVia": {
			"referenceName": "IntegrationRuntime-Azure-ManagedVnet-01",
			"type": "IntegrationRuntimeReference"
		}
	},
	"type": "Microsoft.DataFactory/factories/linkedservices"
}
{
	"name": "LS_FTP",
	"type": "Microsoft.DataFactory/factories/linkedservices",
	"properties": {
		"type": "FtpServer",
		"parameters": {
			"host": {
				"type": "string"
			},
			"username": {
				"type": "string"
			},
			"password": {
				"type": "string"
			}
		},
		"annotations": [],
		"typeProperties": {
			"host": "@{linkedService().host}",
			"port": 21,
			"enableSsl": false,
			"enableServerCertificateValidation": false,
			"authenticationType": "basic",
			"userName": "@{linkedService().username}",
			"password": "@{linkedService().password}"
		}
	}
}
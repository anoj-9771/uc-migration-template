{
	"name": "DeltatableRead",
	"properties": {
		"description": "Copy Activity to read source and write straight to Delta Tables",
		"activities": [
			{
				"name": "Copy-Source-DataLake",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.10:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureDatabricksDeltaLakeSource",
						"query": "select count(1) as RecCount from curated.dimDate",
						"exportSettings": {
							"type": "AzureDatabricksDeltaLakeExportCommand"
						}
					},
					"sink": {
						"type": "AzureDatabricksDeltaLakeSink",
						"importSettings": {
							"type": "AzureDatabricksDeltaLakeImportCommand"
						}
					},
					"enableStaging": true,
					"stagingSettings": {
						"linkedServiceName": {
							"referenceName": "LS_AzureDatalake",
							"type": "LinkedServiceReference"
						},
						"path": "raw/landing/isu_data/test"
					},
					"parallelCopies": 32,
					"dataIntegrationUnits": 8
				},
				"inputs": [
					{
						"referenceName": "DS_RAW_ADLS_DELTALAKE",
						"type": "DatasetReference",
						"parameters": {
							"TableName": "isu_test"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "DS_RAW_ADLS_DELTALAKE",
						"type": "DatasetReference",
						"parameters": {
							"TableName": "isu_test"
						}
					}
				]
			}
		],
		"folder": {
			"name": "z_Archive/Test"
		},
		"annotations": [],
		"lastPublishTime": "2022-03-06T12:36:24Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}
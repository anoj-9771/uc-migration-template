{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-swcnonprod01-daf-dev-01"
		},
		"LS_AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AzureBlobStorage'"
		},
		"LS_AzureDatalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_AzureDatalake'"
		},
		"LS_AzureSynapseAnalytics_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AzureSynapseAnalytics'"
		},
		"LS-AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-daf-dev-01.vault.azure.net/"
		},
		"LS-AzureSqlDatabase_Ctrldb_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "@linkedService().kvSecret"
		},
		"LS_AzureDatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sadafdev01.dfs.core.windows.net"
		},
		"LS_Oracle_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "@linkedService().kvSecret"
		},
		"DLY1300_properties_01-Main_parameters_TriggerName": {
			"type": "string",
			"defaultValue": "DLY1300"
		},
		"Emrvisaf_properties_01-Main_parameters_TriggerName": {
			"type": "string",
			"defaultValue": "Emrvisaf"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-swcnonprod01-daf-dev-01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureBlobStorage_connectionString": {
						"value": "[parameters('LS_AzureBlobStorage_connectionString')]"
					},
					"LS_AzureDatalake_accountKey": {
						"value": "[parameters('LS_AzureDatalake_accountKey')]"
					},
					"LS_AzureSynapseAnalytics_connectionString": {
						"value": "[parameters('LS_AzureSynapseAnalytics_connectionString')]"
					},
					"LS-AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS-AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"LS_AzureDatalake_properties_typeProperties_url": {
						"value": "[parameters('LS_AzureDatalake_properties_typeProperties_url')]"
					},
					"LS_Oracle_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_Oracle_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-swcnonprod01-daf-dev-01_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS-AzureSqlDatabase_Ctrldb_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS-AzureSqlDatabase_Ctrldb_properties_typeProperties_connectionString_secretName')]"
					},
					"DLY1300_properties_01-Main_parameters_TriggerName": {
						"value": "[parameters('DLY1300_properties_01-Main_parameters_TriggerName')]"
					},
					"Emrvisaf_properties_01-Main_parameters_TriggerName": {
						"value": "[parameters('Emrvisaf_properties_01-Main_parameters_TriggerName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-swcnonprod01-daf-dev-01_ArmTemplate_0')]"
			]
		}
	]
}